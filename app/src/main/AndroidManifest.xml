<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hd.tvs.healthdisqus">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:name="com.hd.tvs.healthdisqus.bean"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher2"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.hd.tvs.healthdisqus.MainActivity" />
        <activity android:name="com.hd.tvs.healthdisqus.Login" />
        <activity android:name="com.hd.tvs.healthdisqus.Register" />
        <activity android:name="com.hd.tvs.healthdisqus.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.hd.tvs.healthdisqus.AddTopic" />
        <activity android:name="com.hd.tvs.healthdisqus.AddPost" />
        <activity android:name="com.hd.tvs.healthdisqus.Profile" />
        <activity android:name="com.hd.tvs.healthdisqus.Bookmarks"></activity>
    </application>

</manifest>